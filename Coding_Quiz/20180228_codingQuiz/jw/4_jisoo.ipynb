   "source": [
    "def noOvertime(time,works):\n",
    "    \n",
    "    while time != 0:\n",
    "        works.sort()\n",
    "        works[-1] = works[-1] - 1\n",
    "        time -= 1\n",
    "        \n",
    "    return works"
   ]
  }
